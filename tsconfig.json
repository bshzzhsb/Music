{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./build/",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strictNullChecks": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "removeComments": true,
    "noImplicitAny": true,
    "sourceMap": true
  },
  "include": ["./src/**/*"]
}
